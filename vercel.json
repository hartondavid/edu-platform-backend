{
    "version": 2,
    "builds": [
        {
            "src": "index.mjs",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.mjs"
        }
    ],
    "buildCommand": "npx knex migrate:latest --knexfile knexfile.cjs",
    "env": {
        "NODE_ENV": "production"
    }
}